    "# Garay Carolina del Valle\n",
    "\n",
    "import numpy as np\n",
    "import random \n",
    "\n",
    "x = np.linspace(-4,4,20)\n",
    "sigma=1\n",
    "gauss1= 1 / sigma * 1 / np.sqrt(2*np.pi) * np.exp(-0.5 * (x/sigma)**2)\n",
    "\n",
    "print(gauss1)\n",
    "\n",
    "gauss2 = gauss1.tolist()\n",
    "\n",
    "aleat = np.random.choice(gauss2, 4, False)\n",
    "print(aleat)\n"
   ]
